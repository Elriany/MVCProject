@model IEnumerable<MVCProject.Models.Adminstrator>
@{
    ViewBag.Title = "show";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>showData</h2>
<table class="table table-hover">
    <thead>
        <tr>
            <th>Id</th>
            <th>Name</th>
            <th>Email</th>
            <th>Password<th>
        </tr>
    </thead>
    <tbody>
        @foreach (var admin in Model)
        {
            <tr>
                <td>@admin.id</td>
                <td>@admin.name</td>
                <td>@admin.email</td>
                <td>@admin.password</td>
            </tr>
        }
           
    </tbody>
</table>
